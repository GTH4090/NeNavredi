<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="NeNavredi.MainWindow"
        Title="NeNavredi"
        Loaded="Control_OnLoaded">
    <Grid RowDefinitions="Auto, *">
        <Button Name="ExitBtn" Content="Закрыть" Click="ExitBtn_OnClick" HorizontalAlignment="Right"/>
        <Button Name="LogoutBtn" Content="Выход" Click="LogoutBtn_OnClick" HorizontalAlignment="Left"/>
        
        <ContentControl Name="MainFrame" Grid.Row="1"/>
    </Grid>
</Window>
